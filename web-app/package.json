{
  "name": "@loginov-rocks/firebase-authenticator-web-app",
  "version": "0.2.3",
  "description": "Web App",
  "scripts": {
    "build": "react-scripts build",
    "clean:build": "rimraf docs",
    "clean:coverage": "rimraf coverage",
    "coverage": "npm run clean:coverage && npm run test:coverage",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint --ext ts,tsx src",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:coverage": "cross-env CI=true npm test -- --coverage",
    "test:update": "cross-env CI=true npm test -- --updateSnapshot",
    "typecheck": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loginov-rocks/Firebase-Authenticator.git"
  },
  "author": "Danila Loginov <danila@loginov.rocks> (https://loginov.rocks)",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/loginov-rocks/Firebase-Authenticator/issues"
  },
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^4.7.0",
    "@material-ui/icons": "^4.5.1",
    "firebase": "^7.5.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-firebaseui": "^4.0.0",
    "react-redux": "^7.1.3",
    "react-scripts": "^3.3.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.4",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "0.0.75",
    "uuid": "^7.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "cross-env": "^7.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0",
    "typescript": "^4.2.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
