{
  "name": "firebase-authenticator",
  "version": "0.2.2",
  "description": "Firebase Authenticator",
  "scripts": {
    "build": "react-scripts build",
    "build:release": "npm run check && npm run build && npm run clean:build && cpx \"build/**\" docs",
    "check": "npm run lint && npm run flow && cross-env CI=true npm test",
    "clean:build": "rimraf docs",
    "clean:coverage": "rimraf coverage",
    "coverage": "npm run clean:coverage && npm run test:coverage",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "flow": "flow",
    "lint": "eslint src",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:coverage": "cross-env CI=true npm test -- --coverage",
    "test:update": "cross-env CI=true npm test -- --updateSnapshot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1oginov/Firebase-Authenticator.git"
  },
  "author": "Danila Loginov <danila@loginov.rocks> (https://loginov.rocks)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1oginov/Firebase-Authenticator/issues"
  },
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^4.7.0",
    "@material-ui/icons": "^4.5.1",
    "firebase": "^7.5.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-firebaseui": "^4.0.0",
    "react-redux": "^7.1.3",
    "react-scripts": "^3.2.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.4",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "0.0.75",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "coveralls": "^3.0.8",
    "cpx": "^1.5.0",
    "cross-env": "^6.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "flow-bin": "^0.114.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
