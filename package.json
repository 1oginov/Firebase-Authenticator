{
  "name": "firebase-authenticator",
  "version": "0.2.1",
  "description": "Firebase Authenticator",
  "scripts": {
    "build": "react-scripts build",
    "build:release": "npm run ci && npm run clean:docs && cpx \"build/**\" docs",
    "ci": "npm run lint && npm run flow && cross-env CI=true npm run test:coverage && npm run build",
    "clean:docs": "rimraf docs",
    "coveralls": "npm run test:coverage && cat ./coverage/lcov.info | coveralls",
    "flow": "flow",
    "lint": "eslint src",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:coverage": "npm test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1oginov/Firebase-Authenticator.git"
  },
  "author": "Danila Loginov <danila@loginov.rocks> (https://loginov.rocks)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1oginov/Firebase-Authenticator/issues"
  },
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "firebase": "^6.3.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-firebaseui": "^4.0.0",
    "react-redux": "^7.1.0",
    "react-scripts": "^3.1.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "0.0.75",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.4",
    "cpx": "^1.5.0",
    "cross-env": "^6.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "flow-bin": "^0.107.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "24.8.0",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^3.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
