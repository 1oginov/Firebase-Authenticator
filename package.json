{
  "name": "firebase-authenticator",
  "version": "0.2.1",
  "description": "Firebase Authenticator",
  "scripts": {
    "build": "react-scripts build",
    "build:release": "npm run check && rimraf docs && cpx \"build/**\" docs",
    "check": "npm run lint && npm run flow && cross-env CI=true npm test && npm run build",
    "coveralls": "npm run test:coverage && cat ./coverage/lcov.info | coveralls",
    "flow": "flow",
    "lint": "eslint src",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:coverage": "npm test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1oginov/Firebase-Authenticator.git"
  },
  "author": "Danila Loginov <danila@loginov.rocks> (https://loginov.rocks)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1oginov/Firebase-Authenticator/issues"
  },
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "firebase": "^5.7.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-firebaseui": "^3.1.2",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "0.0.54",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint-config-airbnb": "^17.1.0",
    "flow-bin": "^0.89.0",
    "jest": "^23.6.0",
    "react-test-renderer": "^16.6.3",
    "rimraf": "^2.6.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
